<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<Select id="idSel" items="{/cities}">
		<items>
			<core:Item key="{name}" text="{name}"></core:Item>
		</items>
	</Select>
	<ComboBox id="idComboBox" items="{/cities}">
		<items>
			<core:Item key="{name}" text="{name}"></core:Item>
		</items>
	</ComboBox>
	<MultiComboBox id="idMultiComboBox" items="{/cities}">
		<items>
			<core:Item key="{name}" text="{name}"></core:Item>
		</items>
	</MultiComboBox>
</core:FragmentDefinition>